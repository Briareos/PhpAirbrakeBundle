<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="phpairbrake.apiKey"></parameter>
        <parameter key="phpairbrake.queue"></parameter>
        <parameter key="phpairbrake.client.class">PhpAirbrakeBundle\Airbrake\Client</parameter>
    </parameters>

    <service id="phpairbrake.client" class="%phpairbrake.client.class%">
         <argument type="service" id="service_container" />
    </service>

</container>
